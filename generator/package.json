{
  "name": "generator-racket",
  "version": "1.5.1",
  "description": "Scaffold a universal react application",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir . --ignore /templates",
    "watch": "babel src --watch --out-dir . --ignore /templates",
    "prepublish": "npm run snyk-protect && npm run babel",
    "snyk-protect": "snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohebifar/racket.git"
  },
  "files": [
    "generators",
    "templates"
  ],
  "keywords": [
    "yeoman-generator",
    "react",
    "redux",
    "redux-saga",
    "redux-thunk",
    "universal",
    "isomorphic"
  ],
  "author": "Mohamad Mohebifar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohebifar/racket/issues"
  },
  "homepage": "https://github.com/mohebifar/racket#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "glob": "^7.0.3",
    "lodash": "^4.13.1",
    "racket-template": "^1.3.1",
    "underscore.string": "^3.3.4",
    "yeoman-generator": "^5.0.0",
    "@snyk/protect": "latest"
  },
  "engines": {
    "node": "^4.1.1",
    "npm": "^3.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0"
  },
  "snyk": true
}
